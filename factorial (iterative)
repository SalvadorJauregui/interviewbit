https://codelab.interviewbit.com/problems/factorial/


public class Solution {
    public int trailingZeroes(int a) {
        return trailingZeroes(a,5);
    }
	
    public int trailingZeroes(int a, int n){
        int res = 0;
        for (; a/n > 0; n*=5)
            res += a/n;
        return res;
    }
    
}


